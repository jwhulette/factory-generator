{
    "name": "jwhulette/factory-generator",
    "description": "Generate Laravel 8+ database factories",
    "keywords": [
        "jwhulette",
        "laravel",
        "factory-generator"
    ],
    "homepage": "https://github.com/jwhulette/factory-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer/composer": "^2.0",
        "doctrine/dbal": "^3.1",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "nunomaduro/larastan": "^0.7.6",
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "spatie/ray": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Jwhulette\\FactoryGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jwhulette\\FactoryGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=2G",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix src --allow-risky=yes --config=.php_cs.dist.php",
        "reset": "vendor/bin/phpunit -d --update-snapshots",
        "tests": [
            "vendor/bin/phpstan analyse --memory-limit=2G",
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwhulette\\FactoryGenerator\\FactoryGeneratorServiceProvider"
            ],
            "aliases": {
                "FactoryGenerator": "Jwhulette\\FactoryGenerator\\FactoryGeneratorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
