{
    "name": "jwhulette/factory-generator",
    "description": "Generate Laravel 8+ database factories",
    "keywords": [
        "jwhulette",
        "laravel",
        "factory-generator"
    ],
    "homepage": "https://github.com/jwhulette/factory-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Jwhulette\\FactoryGenerator\\": "src",
            "Jwhulette\\FactoryGenerator\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jwhulette\\FactoryGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwhulette\\FactoryGenerator\\FactoryGeneratorServiceProvider"
            ],
            "aliases": {
                "FactoryGenerator": "Jwhulette\\FactoryGenerator\\FactoryGeneratorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
