{
    "name": "jwhulette/factory-generator",
    "description": "Generate Laravel 8+ database factories",
    "keywords": [
        "jwhulette",
        "laravel",
        "factory-generator"
    ],
    "homepage": "https://github.com/jwhulette/factory-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "composer/composer": "^2.0",
        "doctrine/dbal": "^3.1",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Jwhulette\\FactoryGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jwhulette\\FactoryGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=2G",
        "test": "vendor/bin/phpunit",
        "fix": "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix src --allow-risky=yes --config=.php-cs-fixer.dist.php",
        "update-snapshots": "vendor/bin/phpunit -d --update-snapshots",
        "tests": [
            "vendor/bin/phpstan analyse --memory-limit=2G",
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwhulette\\FactoryGenerator\\FactoryGeneratorServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
